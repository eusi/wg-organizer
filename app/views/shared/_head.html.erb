<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
<head profile="http://www.w3.org/2006/03/hcard">
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
  <title>roomie</title>
  <link rel="shortcut icon" href="favicon.ico" type="image/x-icon">

  <link rel="stylesheet" href='./assets/main.css' media="screen, siteion">  

  <script type="text/javascript" src="./assets/jquery.min.js"></script>
  <script type="text/javascript" src="./assets/main.js"></script>

	<script type="text/javascript" src="./assets/amcharts.js"></script>
	<script type="text/javascript" src="./assets/serial.js"></script>
	<script type="text/javascript" src="./assets/none.js"></script>       

	<script type="text/javascript">
		AmCharts.makeChart("chartdiv",
		{
		    "type": "serial",
			"theme": "none",
		    "dataProvider": [{
		        "name": "Patrick", //!
		        "points": 35654,
		        "color": "#7F8DA9",
		        "bullet": "./assets/test1.png" //!
		    }, {
		        "name": "Chris", //!
		        "points": 65456,
		        "color": "#FEC514",
		        "bullet": "./assets/test3.png" //!
		    }, {
		        "name": "Werner", //!
		        "points": 45724,
		        "color": "#DB4C3C",
		        "bullet": "./assets/test2.png" //!
		    }],
		    "valueAxes": [{
		        "maximum": 80000, //!
		        "minimum": 0,	  //!
		        "axisAlpha": 0,
		        "gridAlpha": 0,
		        "dashLength": 0,
		        "position": "left"
		    }],
		    "startDuration": 1,
		    "graphs": [{
		        "balloonText": "<span style='font-size:13px;'>[[category]]: <b>[[value]]</b></span>",
		        "bulletOffset": 16,
		        "bulletSize": 34,
		        "colorField": "color",
		        "cornerRadiusTop": 8,
		        "customBulletField": "bullet",
		        "fillAlphas": 0.8,
		        "lineAlpha": 0,
		        "type": "column",
		        "valueField": "points",
		        "bulletSize": 100
		    }],
		    "marginTop": 0,
		    "marginRight": 0,
		    "marginLeft": 0,
		    "marginBottom": 0,
		    "autoMargins": false,
		    "categoryField": "name",
		    "categoryAxis": {
		        "axisAlpha": 0,
		        "gridAlpha": 0,
		        "inside": true,
		        "tickLength": 0
		    }
		});
	</script>
</head>